class Solution {
    public int majorityElement(int[] nums) {
        HashMap<Integer, Integer> hash = new HashMap<>();
        int num = 0;
        int most = 0;

        for (int n : nums) {
            hash.put(n, 1 + hash.getOrDefault(n, 0));
            if (hash.get(n) > most) {
                num = n;
                most = hash.get(n);
            }
        }
        return num;
    }
}
