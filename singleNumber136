class Solution {
    public int singleNumber(int[] nums) {
        Arrays.sort(nums);
        
        for (int i = 1; i < nums.length; i += 2) {
            if (!(nums[i] == nums[i - 1])) {
                // 1, 1, 2, 3, 3
                if (nums[i] == nums[i + 1]) {
                    return nums[i - 1];
                } else {
                    return nums[i];
                }
            }
        }
        
        return nums[nums.length - 1];

    }
}
