class Solution {
    public List<String> fizzBuzz(int n) {
        ArrayList<String> answer = new ArrayList<>();
        for (int i = 1; i < n + 1; n++) {
            if (i % 3 == 0 && i % 5 == 0) {
                answer.add(i, "FizzBuzz");
            } else if (i % 3 == 0) {
                answer.add(i, "Fizz");
            } else if (i % 5 == 0) {
                answer.add(i, "Buzz");
            } else {
                answer.add(i, i);
            }
        }
    }
}
